execute pathogen#infect()
syntax enable
set background=dark
set t_Co=256
colorscheme solarized
filetype plugin indent on
set softtabstop=0
set tabstop=2
set shiftwidth=2
set expandtab
set smartindent
set smarttab
let mapleader=','
nmap <Leader>o :set nopaste<CR>
nmap <Leader>p :set paste<CR>
map <Leader>n ik$
map <Leader>tn i  k$
nmap <F8> :TagbarToggle<CR>
nmap <C-o> o<Esc>
